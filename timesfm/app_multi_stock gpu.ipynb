    "def get_weekday(date_str):\n",
    "    date_obj = datetime.strptime(date_str, '%Y-%m-%d')\n",
    "    weekdays = ['1', '2', '3', '4', '5', '6', '0']\n",
    "    return weekdays[date_obj.weekday()]\n",
    "df[\"week_day\"] = df[\"date_str\"].apply(get_weekday)\n",
    "def get_batched_data_fn(\n",
    "    batch_size: int = 128, \n",
    "    context_len: int = 120, \n",
    "    horizon_len: int = 24,\n",
    "):\n",
    "  examples = defaultdict(list)\n",
    "\n",
    "  num_examples = 0\n",
    "  for start in range(0, len(df) - (context_len + horizon_len), horizon_len):\n",
    "    num_examples += 1\n",
    "    examples[\"inputs\"].append(df[\"close\"][start:(context_end := start + context_len)].tolist())\n",
    "    examples[\"open\"].append(df[\"open\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"high\"].append(df[\"high\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"low\"].append(df[\"low\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"volume\"].append(df[\"volume\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"turnover\"].append(df[\"turnover\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"week_day\"].append(df[\"week_day\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"stock_code\"].append(df[\"stock_code\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"amount\"].append(df[\"amount\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"amplitude\"].append(df[\"amplitude\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"percentage_change\"].append(df[\"percentage_change\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"amount_change\"].append(df[\"amount_change\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"open_industry\"].append(df[\"open_industry\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"close_industry\"].append(df[\"close_industry\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"high_industry\"].append(df[\"high_industry\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"low_industry\"].append(df[\"low_industry\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"percentage_industry\"].append(df[\"percentage_industry\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"amount_industry\"].append(df[\"amount_industry\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"amount_change_industry\"].append(df[\"amount_change_industry\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"volume_industry\"].append(df[\"volume_industry\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"turnover_industry\"].append(df[\"turnover_industry\"][start:context_end + horizon_len].tolist())\n",
    "    examples[\"amplitude_industry\"].append(df[\"amplitude_industry\"][start:context_end + horizon_len].tolist())\n",
