    "# 计算MSE， MAE\n",
    "def mean_squared_error(y_pred, y_true):\n",
    "    return np.mean((y_true - y_pred) ** 2)\n",
    "def mean_absolute_error(y_pred, y_true):\n",
    "    return np.mean(np.abs(y_true - y_pred))"
